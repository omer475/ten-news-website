{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "installCommand": "npm install",
  "devCommand": "npm run dev",
  "functions": {
    "pages/api/**/*.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/tennews_data_(.*).json",
      "headers": {
        "Cache-Control": "s-maxage=3600, stale-while-revalidate"
      }
    },
    {
      "src": "/historical_events_(.*).json", 
      "headers": {
        "Cache-Control": "s-maxage=3600, stale-while-revalidate"
      }
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}
